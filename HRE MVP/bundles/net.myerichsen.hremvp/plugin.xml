<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="net.myerichsen.hremvp"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="applicationCSS"
               value="platform:/plugin/net.myerichsen.hremvp/css/default.css">
         </property>
         <property
               name="appName"
               value="net.myerichsen.hremvp">
         </property>
      </product>
   </extension>
   <extension
         point="com.opcoach.e4.preferences.e4PreferencePages">
      <page
            class="net.myerichsen.hremvp.preferences.MvpPreferencePage"
            id="net.myerichsen.hremvp.rootpage"
            name="MVP Preferences">
      </page>
      <page
            category="net.myerichsen.hremvp.rootpage"
            class="net.myerichsen.hremvp.preferences.ClientPreferencePage"
            id="net.myerichsen.hremvp.clientpage"
            name="Client">
      </page>
      <page
            category="net.myerichsen.hremvp.rootpage"
            class="net.myerichsen.hremvp.preferences.H2PreferencePage"
            id="net.myerichsen.hremvp.h2page"
            name="H2">
      </page>
      <page
            category="net.myerichsen.hremvp.rootpage"
            class="net.myerichsen.hremvp.preferences.ServerPreferencePage"
            id="net.myerichsen.hremvp.serverpage"
            name="Server">
      </page>
      <page
            category="net.myerichsen.hremvp.rootpage"
            class="net.myerichsen.hremvp.preferences.WebSearchPreferencePage"
            id="net.myerichsen.hremvp.websearchpage"
            name="Web Sites">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="net.myerichsen.hremvp.preferences.DefaultValuesInitializer">
      </initializer>
   </extension>
   <extension
         id="HRE_MVP"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="net.myerichsen.hremvp">
         <property
               name="appName"
               value="net.myerichsen.hremvp">
         </property>
      </product>
   </extension>

</plugin>
